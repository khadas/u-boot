			function = "pwm_f";
		};
	};

	external_eth_rmii_pins: external_eth_rmii_pins {
		mux {
			groups = "eth_mdio_c",
				"eth_mdc_c",
				"eth_rgmii_rx_clk_c",
				"eth_rx_dv_c",
				"eth_rxd0_c",
				"eth_rxd1_c",
				"eth_txen_c",
				"eth_txd0_c",
				"eth_txd1_c";
			function = "eth";
			drive-strength = <3>;
		};
	};

	external_eth_rgmii_pins: external_eth_rgmii_pins {
		mux {
			groups = "eth_mdio_c",
				"eth_mdc_c",
				"eth_rgmii_rx_clk_c",
				"eth_rx_dv_c",
				"eth_rxd0_c",
				"eth_rxd1_c",
				"eth_rxd2_rgmii_c",
				"eth_rxd3_rgmii_c",
				"eth_rgmii_tx_clk_c",
				"eth_txen_c",
				"eth_txd0_c",
				"eth_txd1_c",
				"eth_txd2_rgmii_c",
				"eth_txd3_rgmii_c";
			function = "eth";
			drive-strength = <3>;
		};
	};
};
