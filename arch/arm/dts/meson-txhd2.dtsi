				"i2cm_a_scl_z7";
			function = "i2cm_a";
			drive-strength = <3>;
			bias-disable;
		};
	};
	i2c1_pins1:i2c1_pins1 {
		mux {
			groups = "i2cm_b_sda_dv0",
				"i2cm_b_scl_dv1";
			function = "i2cm_b";
